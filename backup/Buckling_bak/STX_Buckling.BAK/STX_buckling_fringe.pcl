         	db_get_load_case_id (bk_title(count),bk_lcids(count))
         	res_utl_get_subcases(bk_lcids(count), nsub2, subids)
         	
         	FOR ( j=1 TO nsub2 )
         	   cnt += 1	
         	   SYS_REALLOCATE_ARRAY (rescase_title, 1, cnt )
         	   rescase_title(cnt) = 	bk_title(count)//"_"//str_from_integer(subids(j))
            END FOR
         END IF
      END FOR
      
